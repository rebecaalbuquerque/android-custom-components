<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomCalendar">

        <attr name="customCalendarType" format="integer">
            <enum name="selection" value="0" />
            <enum name="range" value="1" />
        </attr>

        <attr name="customCalendarAvailability" format="integer">
            <enum name="all" value="0" />
            <enum name="beforeCurrentDay" value="1" />
            <enum name="afterCurrentDay" value="2" />
        </attr>

    </declare-styleable>

</resources>